{
  "name": "project-boilerplate-generator",
  "displayName": "Boilerplate Project Setup",
  "description": "VSCode Extension to generate boilerplate code for Web, Mobile, and Backend projects",
  "version": "0.0.1",
  "publisher": "mateen",
  "repository": {
    "type": "git",
    "url": "https://github.com/devsphere-apps/project-boilerplate-generator"
  },
  "entryPoint": "./out/extension.js",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [
    "onCommand:extension.startProjectSetup"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.startProjectSetup",
        "title": "Start Project Setup"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "projectSetupView",
          "name": "AI Project Setup"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/node": "^14.14.31",
    "@types/vscode": "^1.50.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.4.0"
  }
}