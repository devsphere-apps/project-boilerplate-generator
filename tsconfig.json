{
	"compilerOptions": {
	  "target": "ES6",
	  "module": "commonjs",
	  "outDir": "./out",
	  "rootDir": "./src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "types": ["node", "mocha", "vscode"]
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
  }
